			</digest>
			<description>
			</description>
		</inlet>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="INLET_TYPE">
			<digest>
			Result (FFTJIT Matrix)
			</digest>
			<description>
				Result (FFTJIT Matrix)
			</description>
		</outlet>
	</outletlist>

	<!--ARGUMENTS-->

	<objarglist>
